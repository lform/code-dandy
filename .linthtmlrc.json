{
	"attr-name-ignore-regex": false,
	"extends": "@linthtml/linthtml-config-recommended",
	"id-class-ignore-regex": false,
	"line-max-len-ignore-regex": false,
	"maxerr": false,
	"raw-ignore-regex": false,
	"text-ignore-regex": false,
	"rules": {
		"attr-bans": false,
		"attr-quote-style": [true, "double"],
		"button-req-content": true,
		"fig-req-figcaption": false,
		"indent-style": [true, "tabs"],
		"indent-width": [true, 1],
		"label-no-enc-textarea-or-select": true,
		"label-req-for": true,
		"line-end-style": [true, "lf"],
		"line-max-len": [true, 120],
		"img-req-alt": [true, "allownull"],
		"no-surrounding-whitespace": false,
		"spec-char-escape": true,
		"title-max-len": [true ,60]
	}
}
